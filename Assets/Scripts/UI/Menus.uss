    margin-top: 15px;
    margin-bottom: 15px;
    color: rgb(0, 0, 0);
}

.title {
    align-self: stretch;
    -unity-background-scale-mode: scale-to-fit;
    height: 400px;
    margin-top: 50px;
    background-image: url('project://database/Assets/Art/UI/Menus/Title.png?fileID=2800000&guid=5678ba214244daa44b0363b4c271f29e&type=3#Title');
}

