.container {
    align-items: center;
    flex-grow: 1;
}

.verticalContainer {
    flex-direction: row;
}

.button {
    -unity-font-style: bold;
    font-size: 45px;
    padding-top: 45px;
    padding-bottom: 45px;
    background-image: url('project://database/Assets/Art/UI/Menus/bt1.png?fileID=2800000&guid=32829aba5707b2743a0f6dd879d202d5&type=3#bt1');
    -unity-text-align: upper-center;
    background-color: rgba(0, 0, 0, 0);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    transition-duration: 0.5s;
    color: rgb(0, 0, 0);
}

.button:hover {
    background-image: url('project://database/Assets/Art/UI/Menus/bt1F.png?fileID=2800000&guid=5ea658ab029ce6247a49b06db49042f5&type=3#bt1F');
}

.buttonFocus {
